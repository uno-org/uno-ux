{
  "name": "uno-ux",
  "version": "0.0.1",
  "packageManager": "pnpm@6.32.9",
  "description": "A set of completely unstyled, fully accessible, interactive experience focused UI components.",
  "keywords": [
    "Vite",
    "Handless",
    "Tailwind CSS",
    "Unocss"
  ],
  "license": "MIT",
  "author": "zgsgs",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/uno-org/uno-ux.git"
  },
  "scripts": {
    "prepare": "husky install",
    "dev:vue": "pnpm dev -C packages/playground-vue",
    "dev:react": "pnpm dev -C packages/playground-react",
    "build": "pnpm build -C internal/build",
    "preview:vue": "vite preview -C packages/playground-vue",
    "preview:react": "vite preview -C packages/playground-react",
    "lint": "eslint . --ext .vue,.js,.ts,.jsx,.tsx,.json --max-warnings 0 --fix",
    "lint:fix": "lint-staged",
    "test": "vitest test",
    "clear:dist": "rimraf dist"
  },
  "dependencies": {
    "@uno-ux/shared": "workspace:*",
    "@uno-ux/vue": "workspace:*"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.4",
    "@commitlint/config-conventional": "^16.2.4",
    "@vue/tsconfig": "^0.1.3",
    "@zgsgs/eslint-config": "^0.0.6",
    "eslint": "^8.14.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.4.1",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "stylelint": "^14.8.1",
    "typescript": "^4.6.4",
    "vitest": "^0.10.1"
  },
  "lint-staged": {
    "*.{vue,js,jsx,ts,tsx,json}": "eslint --fix"
  },
  "bugs": {
    "url": "https://github.com/uno-org/uno-ux/issues"
  },
  "homepage": "https://github.com/uno-org/uno-ux#readme"
}
